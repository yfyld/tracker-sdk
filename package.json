{
  "name": "@zyf2e/telescope",
  "version": "1.0.28",
  "description": "",
  "main": "dist/telescope.min.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "rollup -c -w",
    "dev-analyse": "rollup -c ./rollup.analyse.config.js -w",
    "analyse": "rollup -c ./rollup.analyse.config.js --environment NODE_ENV:production",
    "build": "rollup -c --environment NODE_ENV:production",
    "build:qa": "rollup -c --environment NODE_ENV:production",
    "publish:": "npm publish",
    "test": "./node_modules/.bin/nightwatch --env",
    "test-build": "rollup -c ./rollup.oneClass.config.js -w",
    "prepublish:": "npm run build",
    "release": "release-it"
  },
  "release-it": {
    "git": {
      "commitArgs": [
        "--no-verify"
      ]
    },
    "npm": {
      "publish": true
    }
  },
  "dependencies": {
    "ramda": "^0.27.0",
    "tslib": "^1.11.1",
    "typescript": "^3.4.2",
    "vissense": "^0.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/preset-env": "^7.4.3",
    "@types/node": "^11.13.0",
    "@types/ramda": "^0.27.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "lodash": "^4.17.11",
    "nightwatch": "^1.0.19",
    "rollup": "^1.21.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-replace": "^2.1.1",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript2": "^0.20.1",
    "rollup-plugin-visualizer": "^3.3.2",
    "vue": "^2.6.10"
  },
  "author": "",
  "license": "ISC"
}
